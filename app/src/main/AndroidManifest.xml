<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cmeb.cnm.firstassignment">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/main_image"
        android:label="@string/Currency"
        android:roundIcon="@drawable/main_image"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CurrencyConversionApp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Taxes_check">
            <intent-filter>
                <action android:name="second_filter" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>